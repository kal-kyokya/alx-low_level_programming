#include <stdio.h>

/**
 *main - Entry point to the function.
 *
 *return: 0 if program runs successfully.
 */
int main(void)
{
	int counter, a, b, c;

	a = 1;
	b = 2;
	c = a + b;
	counter = 0;
	while (counter < 48)
	{
		c = a + b;
		printf("%d, ", c);
		a = b;
		b = c;
		counter++;
	}
	return (0);
}
