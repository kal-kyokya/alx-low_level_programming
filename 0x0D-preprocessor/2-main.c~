#include "main.h"
#include <stdlib.h>

/**
 * main - Entry point.
 *
 * Return: Nothing.
 */
void main(void)
{
	char *s;
	int strlen, counter;

	s = __FILE__;
	strlen = counter = 0;
	while (*(s + strlen) != '\0')
		strlen++;
	while (counter < strlen)
	{
		_putchar(*(s + counter));
		counter++;
	}
	_putchar('\n');
}
