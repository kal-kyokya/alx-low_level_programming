#include <stdio.h>

/**
 *main - Entry point to the program.
 *
 *Return: 0 if program runs successfully.
 */
int main(void)
{
	int x;

	x = 0;
	while (x <= 9)
	{
		if (x == 9)
		{
			putchar(x + '0');
			x++;
	}
		else
		{
			putchar(x + '0');
			putchar(',');
			putchar(' ');
			x++;
		}
	}
	return (0);
}
